%function demo_SR(iters)


t = cputime;

%% read ground truth image
im  = imread('miku_bg.png');

%% work on illuminance only
if size(im,3)>1
    im = rgb2ycbcr(im);
    im = im(:, :, 1);
end

im_gnd = single(im)/255;

im  = imread('/home/chunyang/Downloads/caffe-rc3/examples/sr_21_1_3x/Test/Set5/baby_GT.bmp');

%% work on illuminance only
if size(im,3)>1
    im = rgb2ycbcr(im);
    im = im(:, :, 1);
end
im_output = single(im)/255;
%% bicubic interpolation


psnr=compute_psnr(im_gnd,im_output)



